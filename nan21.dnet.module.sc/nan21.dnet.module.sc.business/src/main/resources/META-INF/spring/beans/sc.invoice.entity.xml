<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"	 
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">   
	<bean id="PurchaseInvoiceService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.sc.invoice.business.serviceext.PurchaseInvoiceService"/>
	<bean id="PurchaseInvoiceItemService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.sc.invoice.business.serviceext.PurchaseInvoiceItemService"/>
	<bean id="PurchaseInvoiceTaxService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.sc.invoice.business.serviceimpl.PurchaseInvoiceTaxService"/>
	<bean id="PurchaseInvoiceItemTaxService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.sc.invoice.business.serviceimpl.PurchaseInvoiceItemTaxService"/>
	<bean id="PaymentOutService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.sc.invoice.business.serviceext.PaymentOutService"/>
	<bean id="PaymentOutAmountService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.sc.invoice.business.serviceimpl.PaymentOutAmountService"/>
	<bean id="PurchaseTxAmountService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.sc.invoice.business.serviceext.PurchaseTxAmountService"/>
