<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="ProjectService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.pj.md.business.serviceimpl.ProjectService"/>				

	<bean id="ProjectMemberService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.pj.md.business.serviceimpl.ProjectMemberService"/>				

	<bean id="ProjectComponentService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.pj.md.business.serviceimpl.ProjectComponentService"/>				

	<bean id="ProjectVersionService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.pj.md.business.serviceimpl.ProjectVersionService"/>				

	<bean id="IssueService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.pj.md.business.serviceimpl.IssueService"/>				

	<bean id="IssueTaskService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.pj.md.business.serviceimpl.IssueTaskService"/>				

	<bean id="IssueTextService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.pj.md.business.serviceimpl.IssueTextService"/>				

	<bean id="IssueLinkService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.pj.md.business.serviceimpl.IssueLinkService"/>				
</beans>
