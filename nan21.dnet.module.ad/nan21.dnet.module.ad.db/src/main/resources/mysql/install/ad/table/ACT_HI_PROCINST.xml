<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="amathe" id="TBL">
        <createTable tableName="ACT_HI_PROCINST">
		<column name="BUSINESS_KEY_" type="varchar(255)" />
		<column name="DELETE_REASON_" type="text" />
		<column name="DURATION_" type="bigint(19)" />
		<column name="END_ACT_ID_" type="varchar(255)" />
		<column name="END_TIME_" type="datetime" />
		<column name="ID_" type="varchar(64)">
			<constraints nullable="false" /></column>
		<column name="PROC_INST_ID_" type="varchar(64)" />
		<column name="START_ACT_ID_" type="varchar(255)" />
		<column name="START_TIME_" type="datetime">
			<constraints nullable="false" /></column>
		<column name="START_USER_ID_" type="varchar(32)" />
		<column name="SUPER_PROCESS_INSTANCE_ID_" type="varchar(255)" />
		<column name="PROC_DEF_ID_" type="varchar(64)">
			<constraints  /></column>
        </createTable>
    </changeSet>

    <changeSet author="amathe" id="PK">
    	<addPrimaryKey tableName="ACT_HI_PROCINST" constraintName="PK_ACT_HI_PROCINST" columnNames="ID_" />
    </changeSet>

	<changeSet author="amathe" id="SEQ">
		<insert tableName="SEQUENCE">
		    <column name="SEQ_NAME" value="ACT_HI_PROCINST_SEQ"/>
		    <column name="SEQ_COUNT" valueNumeric="0"/>
		</insert>
	</changeSet>
</databaseChangeLog>
