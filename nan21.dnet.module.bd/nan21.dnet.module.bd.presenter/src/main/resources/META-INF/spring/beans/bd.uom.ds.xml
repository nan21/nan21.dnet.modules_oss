<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">
	
	<!-- ==================== UomType ==================== -->
	
	<bean id="UomTypeDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.bd.uom.ds.model.UomTypeDs"/>
		<property name="filterClass" value="net.nan21.dnet.module.bd.uom.ds.model.UomTypeDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.bd.uom.domain.entity.UomType"/>
	</bean>
	
