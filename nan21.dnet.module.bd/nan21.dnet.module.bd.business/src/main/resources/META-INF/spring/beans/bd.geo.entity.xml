<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"	 
	xmlns:int="http://www.springframework.org/schema/integration"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd
			http://www.springframework.org/schema/integration
				http://www.springframework.org/schema/integration/spring-integration.xsd">

	<bean id="CountryService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.bd.geo.business.serviceimpl.CountryService"/>

	<bean id="RegionTypeService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.bd.geo.business.serviceimpl.RegionTypeService"/>

	<bean id="RegionService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.bd.geo.business.serviceimpl.RegionService"/>

	<bean id="CityService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.bd.geo.business.serviceimpl.CityService"/>

	<bean id="LocationService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.bd.geo.business.serviceimpl.LocationService"/>

	<bean id="GeoZoneTypeService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.bd.geo.business.serviceimpl.GeoZoneTypeService"/>

	<bean id="GeoZoneService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.bd.geo.business.serviceimpl.GeoZoneService"/>
</beans>
