<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">
	
	<!-- ==================== CourseCategory ==================== -->
	
	<bean id="CourseCategoryDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.hr.training.ds.model.CourseCategoryDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.hr.training.domain.entity.CourseCategory"/>
	</bean>
	
	<!-- ==================== CourseCategoryLov ==================== -->
	
	<bean id="CourseCategoryLovDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.hr.training.ds.model.CourseCategoryLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.hr.training.domain.entity.CourseCategory"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== CourseType ==================== -->
	
	<bean id="CourseTypeDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.hr.training.ds.model.CourseTypeDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.hr.training.domain.entity.CourseType"/>
		<property name="converterClass" value="net.nan21.dnet.module.hr.training.ds.converter.CourseTypeDsConv" />
	</bean>
	
	<!-- ==================== CourseTypeLov ==================== -->
	
	<bean id="CourseTypeLovDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.hr.training.ds.model.CourseTypeLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.hr.training.domain.entity.CourseType"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== Course ==================== -->
	
	<bean id="CourseDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.hr.training.ds.model.CourseDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.hr.training.domain.entity.Course"/>
		<property name="converterClass" value="net.nan21.dnet.module.hr.training.ds.converter.CourseDsConv" />
	</bean>
	
	<!-- ==================== CourseLov ==================== -->
	
	<bean id="CourseLovDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.hr.training.ds.model.CourseLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.hr.training.domain.entity.Course"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== JobCourse ==================== -->
	
	<bean id="JobCourseDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.hr.training.ds.model.JobCourseDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.hr.training.domain.entity.JobCourse"/>
		<property name="converterClass" value="net.nan21.dnet.module.hr.training.ds.converter.JobCourseDsConv" />
	</bean>
	
	<!-- ==================== PositionCourse ==================== -->
	
	<bean id="PositionCourseDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.hr.training.ds.model.PositionCourseDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.hr.training.domain.entity.PositionCourse"/>
		<property name="converterClass" value="net.nan21.dnet.module.hr.training.ds.converter.PositionCourseDsConv" />
	</bean>
</beans>
