		class="net.nan21.dnet.module.hr.time.business.serviceimpl.AbsenceReasonService"/>

	<bean id="AbsenceRequestService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.hr.time.business.serviceimpl.AbsenceRequestService"/>

	<bean id="AbsenceRequestItemService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.hr.time.business.serviceimpl.AbsenceRequestItemService"/>

	<bean id="AbsenceService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.hr.time.business.serviceimpl.AbsenceService"/>

