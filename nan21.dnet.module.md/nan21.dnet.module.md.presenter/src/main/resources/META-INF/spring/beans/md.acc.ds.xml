<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">
	
	<!-- ==================== AccSchema ==================== -->
	
	<bean id="AccSchemaDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.acc.ds.model.AccSchemaDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.acc.domain.entity.AccSchema"/>
	</bean>
	
	<!-- ==================== AccSchemaLov ==================== -->
	
	<bean id="AccSchemaLovDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.acc.ds.model.AccSchemaLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.acc.domain.entity.AccSchema"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== Account ==================== -->
	
	<bean id="AccountDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.acc.ds.model.AccountDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.acc.domain.entity.Account"/>
		<property name="converterClass" value="net.nan21.dnet.module.md.acc.ds.converter.AccountDsConv" />
	</bean>
	
	<!-- ==================== AccountLov ==================== -->
	
	<bean id="AccountLovDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.acc.ds.model.AccountLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.acc.domain.entity.Account"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== AccItem ==================== -->
	
	<bean id="AccItemDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.acc.ds.model.AccItemDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.acc.domain.entity.AccItem"/>
	</bean>
	
	<!-- ==================== AccItemLov ==================== -->
	
	<bean id="AccItemLovDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.acc.ds.model.AccItemLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.acc.domain.entity.AccItem"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== AccItemAcct ==================== -->
	
	<bean id="AccItemAcctDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.acc.ds.model.AccItemAcctDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.acc.domain.entity.AccItemAcct"/>
		<property name="converterClass" value="net.nan21.dnet.module.md.acc.ds.converter.AccItemAcctDsConv" />
	</bean>
	
	<!-- ==================== AccJournal ==================== -->
	
	<bean id="AccJournalDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.acc.ds.model.AccJournalDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.acc.domain.entity.AccJournal"/>
	</bean>
	
	<!-- ==================== AccJournalLov ==================== -->
	
	<bean id="AccJournalLovDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.acc.ds.model.AccJournalLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.acc.domain.entity.AccJournal"/>
		<property name="readOnly" value="true" />
	</bean>
</beans>
