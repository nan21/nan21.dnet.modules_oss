		<property name="modelClass" value="net.nan21.dnet.module.md.org.ds.model.StockLocatorTypeLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.org.domain.entity.StockLocatorType"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== StockLocator ==================== -->
	
	<bean id="StockLocatorDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.org.ds.model.StockLocatorDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.org.domain.entity.StockLocator"/>
		<property name="converterClass" value="net.nan21.dnet.module.md.org.ds.converter.StockLocatorDsConv" />
