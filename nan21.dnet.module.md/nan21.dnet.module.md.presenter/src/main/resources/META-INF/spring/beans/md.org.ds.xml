<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">
	
	<!-- ==================== FinancialAccount ==================== -->
	
	<bean id="FinancialAccountDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.org.ds.model.FinancialAccountDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.org.domain.entity.FinancialAccount"/>
		<property name="converterClass" value="net.nan21.dnet.module.md.org.ds.converter.FinancialAccountDsConv" />
	</bean>
	
	<!-- ==================== FinancialAccountLov ==================== -->
	
	<bean id="FinancialAccountLovDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.org.ds.model.FinancialAccountLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.org.domain.entity.FinancialAccount"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== FinancialAccountAcct ==================== -->
	
	<bean id="FinancialAccountAcctDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.org.ds.model.FinancialAccountAcctDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.org.domain.entity.FinancialAccountAcct"/>
		<property name="converterClass" value="net.nan21.dnet.module.md.org.ds.converter.FinancialAccountAcctDsConv" />
	</bean>
	
	<!-- ==================== FinancialAccountMethod ==================== -->
	
	<bean id="FinancialAccountMethodDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.org.ds.model.FinancialAccountMethodDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.org.domain.entity.FinancialAccountMethod"/>
		<property name="converterClass" value="net.nan21.dnet.module.md.org.ds.converter.FinancialAccountMethodDsConv" />
	</bean>
	
	<!-- ==================== SubInventory ==================== -->
	
	<bean id="SubInventoryDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.org.ds.model.SubInventoryDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.org.domain.entity.SubInventory"/>
		<property name="converterClass" value="net.nan21.dnet.module.md.org.ds.converter.SubInventoryDsConv" />
	</bean>
	
	<!-- ==================== SubInventoryLov ==================== -->
	
	<bean id="SubInventoryLovDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.org.ds.model.SubInventoryLovDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.org.domain.entity.SubInventory"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== StockLocatorType ==================== -->
	
	<bean id="StockLocatorTypeDsService" scope="singleton" lazy-init="true"
			class="net.nan21.dnet.core.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="net.nan21.dnet.module.md.org.ds.model.StockLocatorTypeDs"/>
		<property name="entityClass" value="net.nan21.dnet.module.md.org.domain.entity.StockLocatorType"/>
	</bean>
	
	<!-- ==================== StockLocatorTypeLov ==================== -->
	
	<bean id="StockLocatorTypeLovDsService" scope="singleton" lazy-init="true"
