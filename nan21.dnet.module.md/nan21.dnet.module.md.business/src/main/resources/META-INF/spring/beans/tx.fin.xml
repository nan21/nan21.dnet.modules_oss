<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="PaymentService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.tx.fin.business.serviceimpl.PaymentService"/>				

	<bean id="PaymentItemService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.tx.fin.business.serviceext.PaymentItemService"/>				

	<bean id="PaymentAmountService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.tx.fin.business.serviceext.PaymentAmountService"/>				

	<bean id="TxAmountService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.tx.fin.business.serviceimpl.TxAmountService"/>				

	<bean id="TxBalanceService" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.tx.fin.business.serviceimpl.TxBalanceService"/>				
</beans>
